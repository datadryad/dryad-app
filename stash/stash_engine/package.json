{
  "name": "dash-ui",
  "version": "1.0.0",
  "description": "A library of static themes, components, and objects for the Dryad application.",
  "main": "index.html",
  "scripts": {
    "copy-pkg-files": "copyfiles -f --error node_modules/dialog-polyfill/dist/dialog-polyfill.css ui-library/css/vendor && copyfiles -f --error node_modules/dialog-polyfill/dist/dialog-polyfill.js ui-library/js/vendor",
    "publish": "gh-pages --dist dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.8",
    "browser-sync": "^2.26.7",
    "browserify": "^17.0.0",
    "browsersync-ssi": "^0.2.4",
    "copyfiles": "^2.4.1",
    "del": "^4.1.1",
    "dialog-polyfill": "^0.5.6",
    "fsevents": "^2.0.7",
    "gh-pages": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.0.0",
    "gulp-jshint": "^2.1.0",
    "gulp-lb-include": "^0.3.1",
    "gulp-modernizr": "^3.4.0",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-shell": "^0.8.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-useref": "^4.0.0",
    "gulp-w3cjs": "^1.3.2",
    "jshint": "^2.12.0",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "postcss-assets": "^5.0.0",
    "run-sequence": "^2.2.1",
    "stylelint": "^13.11.0",
    "stylelint-config-property-sort-order-smacss": "^7.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0"
  },
  "browser-sync": "^2.26.14",
  "del": "^4.1.1",
  "fsevents": "^2.3.1",
  "jshint": "^2.12.0",
  "stylelint": {
    "defaultSeverity": "warning",
    "extends": [
      "stylelint-config-recommended-scss",
      "stylelint-config-property-sort-order-smacss"
    ],
    "rules": {
      "max-nesting-depth": [
        4,
        {
          "ignore": [
            "blockless-at-rules",
            "pseudo-classes"
          ]
        }
      ],
      "no-descending-specificity": null
    }
  }
}
