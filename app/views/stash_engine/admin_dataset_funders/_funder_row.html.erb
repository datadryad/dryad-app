<% ds_row = funder_row %>
<tr class="c-lined-table__row" id="js-dataset-row-id-<%= ds_row.id %>">
  <td>
    <% if ds_row.title.blank? %>
      <%= ds_row.title %>
    <% else %>
      <%= link_to ds_row.title, stash_url_helpers.show_path(id: "doi:#{ds_row.identifier}"), target: '_blank' %>
    <% end %>
  </td>
  <td>
    <%= ds_row.authors %>
  </td>
  <td class="c-lined-table__digits">
    <%= ds_row.identifier %>
  </td>
  <td>
    <%= ds_row.contributor_name %>
  </td>
  <td>
    <%= ds_row.award_number %>
  </td>
  <td class="c-lined-table__digits">
    <%= formatted_datetime(ds_row.init_sub_date) %>
  </td>
  <td class="c-lined-table__digits">
    <% if ds_row.pub_state == 'embargoed' && ds_row.publication_date && ds_row.publication_date > Time.new %>
      <%= formatted_datetime(ds_row.publication_date) %>
    <% end %>
  </td>
  <td class="c-lined-table__digits">
    <% if ds_row.pub_state == 'published' && ds_row.publication_date && ds_row.publication_date < Time.new %>
      <%= formatted_datetime(ds_row.publication_date) %>
    <% end %>
  </td>
</tr>
