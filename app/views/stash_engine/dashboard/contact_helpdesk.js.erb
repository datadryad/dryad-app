document.getElementById('genericModalContent').innerHTML = '';
document.getElementById('genericModalContent').append(document.createRange().createContextualFragment("<h1>Contact the helpdesk</h1><%= escape_javascript(render partial: 'stash_engine/shared/contact_form', locals: {identifier: params[:id]}) %><p class='c-modal__buttons-right'><button id='cancel_dialog' class= 'o-button__plain-text7'>Close</button></p>"));
document.getElementById('genericModalDialog').showModal();
document.getElementById('cancel_dialog').addEventListener('click', (e) => {
  e.preventDefault();
  document.getElementById('genericModalDialog').close();
});