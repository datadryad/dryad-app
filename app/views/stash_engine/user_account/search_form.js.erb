document.getElementById('admin_searches_list').classList.add('with_form')
document.getElementById('saved_search<%= @saved_search.id %>').innerHTML = "<%= escape_javascript(render partial: 'stash_engine/user_account/edit_search', locals: { saved_search: @saved_search }) %>";

document.getElementById('cancel_save<%= @saved_search.id %>').addEventListener('click', (e) => {
  e.preventDefault()
  document.getElementById('saved_search<%= @saved_search.id %>').outerHTML = "<%= escape_javascript(render partial: 'stash_engine/user_account/saved_search', locals: { saved_search: @saved_search, index: @index }) %>";
})