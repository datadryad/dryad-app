document.getElementById("user_flag_<%= @user.id %>").innerHTML = <% if @user.flag.present? %>`<i class="fas fa-flag user" aria-label="Flagged" title="<%= @user.flag.note %>"></i>`<% else %>""<% end %>
document.getElementById("user_email_<%= @user.id %>").innerHTML = "<%= @user.email %>"
document.getElementById("user_tenant_id_<%= @user.id %>").innerHTML = "<%= @user.tenant.short_name %>"
if (document.getElementById('user_role_<%= @user.id %>')) {
  document.getElementById('user_role_<%= @user.id %>').innerHTML = "<%= role_list(@user) %>";
}
if (document.getElementById('user_role_details')) {
  document.getElementById('user_role_details').innerHTML = "<%= role_details(@user) %>";
}
document.getElementById('genericModalDialog').close();