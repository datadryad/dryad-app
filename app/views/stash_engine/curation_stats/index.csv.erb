<%= "Date,Curated,QueueSize,New,NewToSubmitted,NewToPPR,PPRToCuration,CurationToAAR,CurationToPublished,Embargoed,Withdrawn,AuthorRevised,AuthorVersioned" %>
<% @all_stats.each do |stat|
    row = [
	formatted_date(stat.date),
	stat.datasets_curated,
	stat.datasets_to_be_curated,
	stat.new_datasets_to_submitted.to_i + stat.new_datasets_to_peer_review.to_i,
	stat.new_datasets_to_submitted,
	stat.new_datasets_to_peer_review,
        stat.ppr_to_curation,
        stat.datasets_to_aar,
        stat.datasets_to_published,
        stat.datasets_to_embargoed,
      	stat.datasets_to_withdrawn,
	stat.author_revised,
	stat.author_versioned 
    ]
-%><%= row.to_csv(row_sep: nil).html_safe %>
<% end -%>
