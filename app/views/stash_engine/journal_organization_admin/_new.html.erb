<h1>Enter a new publisher</h1>
<%= form_with(url: publisher_create_path, method: :post, local: true) do |form| -%>
  <%= render partial: 'name', locals: {form: form} %>
  <%= render partial: 'contact', locals: {form: form} %>
  <%= render partial: 'parent_org_id', locals: {form: form} %>
  <div class="c-modal__buttons-right">
    <%= submit_tag 'Submit', class: 'o-button__plain-text2' %>
    <%= button_tag 'Cancel', type: 'button', id: 'cancel_dialog', class: 'o-button__plain-text7' %>
  </div>
<% end %>