<%= render partial: 'stash_engine/shared/dataset_non_user_editor', locals: {resource: @resource} %>
<%= react_component('containers/Submission', {
  submission: @submission,
  user: {id: current_user.id, orcid: current_user.orcid, curator: current_user.min_curator?, superuser: current_user.superuser?},
  s3_dir_name: @resource.s3_dir_name(type: 'base'),
  config_s3: APP_CONFIG[:s3].to_h.except(:secret).to_ostruct,
  config_maximums: APP_CONFIG[:maximums].to_h,
  config_payments: APP_CONFIG[:payments].to_h.except(:key),
  change_tenant: render(partial: 'stash_engine/shared/change_tenant')
}) %>
