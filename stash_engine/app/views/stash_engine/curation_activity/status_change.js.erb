$("#js-curation-state-<%= params[:id] %>").html("<%= @activity.readable_status %>");
$("#js-embargo-state-<%= params[:id] %>").html("<%= formatted_date(@resource.publication_date) %>");
$("#popup_dialog").dialog("close");
<% if params[:log_page] == 'true' %>
  if($("#curation_table .c-admin-table__sort-asc").length > 0){
      $('#curation_table .c-lined-table__row:last').after("<%= escape_javascript(
          render partial: 'stash_engine/admin_datasets/activity_log_row', locals: { curation_activity: @activity }) %>");
  }else{
      $('#curation_table .c-lined-table__head').after("<%= escape_javascript(
          render partial: 'stash_engine/admin_datasets/activity_log_row', locals: { curation_activity: @activity }) %>");
  }
<% end %>