{
  "name": "dryad",
  "jest": {
    "verbose": true,
    "roots": [
      "spec/javascript"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/javascript"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "^@milkdown/kit/prose$": "<rootDir>/node_modules/@milkdown/prose/lib/index.js",
      "^@milkdown/kit/(.*)$": "<rootDir>/node_modules/@milkdown/kit/lib/$1.js",
      "^@milkdown/prose/(.*)$": "<rootDir>/node_modules/@milkdown/prose/lib/$1.js",
      "^@milkdown/(.*)$": "<rootDir>/node_modules/@milkdown/$1"
    },
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(nanoid/.*)",
      "node_modules/htmldiff-js/.*"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/spec/javascript/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "resources": "usable"
    }
  },
  "scripts": {
    "start": "bin/webpack serve --mode development",
    "build": "webpack",
    "test": "jest",
    "index:help": "yarn pagefind --site public/cache --output-path public/pagefind --exclude-selectors 'a.emailr'",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .jsx,.js */javascript/react --fix",
    "lint:style": "stylelint app/**/*.scss app/**/*.css --fix"
  },
  "dependencies": {
    "@babel/core": "7",
    "@babel/eslint-parser": "^7.18.6",
    "@babel/plugin-transform-runtime": "7",
